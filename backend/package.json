{
  "name": "jsg-inspections-backend",
  "version": "1.0.0",
  "description": "Backend API for JSG-Inspections application - A comprehensive inspection management system",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "npx swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o docs/swagger.json",
    "docs:yaml": "npx swagger-jsdoc -d swaggerDef.js src/routes/*.ts -o docs/swagger.yaml",
    "docs:all": "npm run docs && npm run docs:yaml",
    "migrate": "ts-node src/database/migrations/migrate.ts",
    "seed": "ts-node src/database/seeds/seed.ts"
  },
  "keywords": [
    "inspection",
    "management",
    "api",
    "surrealdb",
    "typescript",
    "express",
    "jwt",
    "websocket"
  ],
  "author": "JSG Development Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "surrealdb.js": "^0.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5",
    "socket.io": "^4.7.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "sharp": "^0.32.5",
    "node-cron": "^3.0.2",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "openai": "^4.0.0",
    "@tensorflow/tfjs-node": "^4.10.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/multer": "^1.4.7",
    "@types/cors": "^2.8.13",
    "@types/node": "^20.4.5",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "@types/morgan": "^1.9.4",
    "@types/uuid": "^9.0.2",
    "@types/compression": "^1.7.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}